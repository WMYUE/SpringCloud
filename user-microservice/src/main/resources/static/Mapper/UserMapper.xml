<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaojie.Dao.UserMapper">
    <select id="selectUserInfoById" parameterType="string" resultType="com.xiaojie.model.UserInfo1">
        SELECT *
        FROM USER
        WHERE userid=#{id}
    </select>
</mapper>